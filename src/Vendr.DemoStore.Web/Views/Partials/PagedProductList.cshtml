@using Vendr.DemoStore;
@using Vendr.DemoStore.Models;
@using Vendr.Common.Models;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<PagedResult<ProductPage>>

@if (Model.TotalItems > 0)
{
    @await Html.PartialAsync("ProductList", Model.Items)
    @await Html.PartialAsync("Pagination", Model.Cast<ProductPage, object>())
}