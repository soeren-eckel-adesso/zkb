@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<IEnumerable<Vendr.DemoStore.Models.ProductPage>>

@if (Model != null)
{
    <div class="flex flex-wrap -mx-3">
        @foreach (var product in Model)
        {
            @await Html.PartialAsync("ProductListItem", product);
        }
    </div>
}